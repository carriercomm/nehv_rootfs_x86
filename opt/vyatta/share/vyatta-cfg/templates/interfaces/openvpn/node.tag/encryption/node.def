type: txt
help: Data encryption algorithm option
syntax:expression: $VAR(@) in "des", "3des", "bf128", "bf256", "aes128", "aes192", "aes256";
                   "Invalid encryption algorithm"
val_help: des; DES algorithm
val_help: 3des; DES algorithm with triple encryption
val_help: bf128; Blowfish algorithm with 128-bit key
val_help: bf256; Blowfish algorithm with 256-bit key
val_help: aes128; AES algorithm with 128-bit key
val_help: aes192; AES algorithm with 192-bit key
val_help: aes256; AES algorithm with 256-bit key
